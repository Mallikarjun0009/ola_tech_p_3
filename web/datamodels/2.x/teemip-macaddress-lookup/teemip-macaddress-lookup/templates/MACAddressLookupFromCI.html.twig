{% apply spaceless %}

{% UIContentBlock Standard {} %}

	{% if bIssue %}

		{% UIAlert ForFailure {} %}
			{% UIContentBlock Standard {} %}
				{{ sMessage }}
			{% EndUIContentBlock %}
		{% EndUIAlert %}

	{%  endif %}

	{% UIPanel Neutral {'sClass':sClass, 'sTitle':'UI:MACLookup:Action:DoLookup:Result'|dict_s, 'sSubTitle':'' } %}
		{% for sMACAddress, aMACResults in aResults %}

			{% include 'DataBlock/MACAddressReport.html.twig' %}

		{% endfor %}
	{% EndUIPanel %}


{% EndUIContentBlock %}

{% endapply %}